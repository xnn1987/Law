<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
   <include 
     android:id="@+id/detail_head_layout"
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
     layout="@layout/law_detail_head"
     android:layout_alignParentTop="true"
      />     
   <RelativeLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
        >
        <EditText 
    android:id="@+id/user_edt"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:hint="请输入用户名"
    android:text="xnn1987"    
            />
            <EditText 
    android:id="@+id/psw_edt"
    android:layout_below="@id/user_edt"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:password="true"
    android:hint="fffffffffff"
    android:text="xnn69307230"    
            />    
    </RelativeLayout>
<TableLayout
    android:id="@+id/remember_tab"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" 
    >
   <TableRow
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"   
         >
    <CheckBox 
    android:id="@+id/user_box"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" 
    android:layout_gravity="center"  
        />

    <TextView
        android:id="@+id/remember_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"  
        android:text="记住我" />

    <TextView 
    android:id="@+id/forget_txt"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center" 
    android:text="忘记密码???"
    android:layout_marginLeft="80sp"
    />
</TableRow>
</TableLayout>
<Button 
    android:id="@+id/login_btn"
    android:layout_marginTop="10sp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:text="登       陆"   
    />
<LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content" 
    android:layout_marginTop="80sp" 
    android:orientation="vertical"
    >
 <TextView 
    android:id="@+id/commend_txt"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"  
    android:text="你可以使用合作账号登陆："   
     />
 <Button 
    android:id="@+id/qq_login_btn"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:text="用QQ账号登陆"
     /> 
      <Button 
    android:id="@+id/sina_login_btn"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:text="用新浪微博账号登陆"
     /> 
      <Button 
    android:id="@+id/tencet_login_btn"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:text="用腾讯微博账号登陆"
     /> 
</LinearLayout>
</LinearLayout>