<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#ffffff" >
  <TextView 
     android:id="@+id/new_fagui_txt"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" 
    android:text="@string/new_fagui"
    android:textSize="16sp"
     />   
 <TextView 
    android:id="@+id/hot_fagui_txt"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" 
    android:text="@string/hot_fagui"
    android:textSize="16sp" 
     />
  <TextView 
    android:id="@+id/native_fagui_txt"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" 
    android:text="@string/native_fagui"
    android:textSize="16sp"
     />
   <TextView 
    android:id="@+id/shisshi_fagui_txt"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" 
    android:text="@string/shishi_fagui"
    android:textSize="16sp"
     />
    <TextView 
    android:id="@+id/shixiao_fagui_txt"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" 
    android:text="@string/shixiao_fagui"
    android:textSize="16sp"
     />   

</LinearLayout>