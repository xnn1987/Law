<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   >
     
  <include 
     android:id="@+id/detail_head_layout"
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
     layout="@layout/law_detail_head"
     android:layout_alignParentTop="true"
      />  
    
<TableRow 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >
    <TextView 
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="反馈类型" 
        />
    <CheckBox 
    android:id="@+id/fagui_error_box"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"   
        />
        <TextView 
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="法规错误" 
        />
        <CheckBox 
    android:id="@+id/faguti_out_box"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"   
        />
                <TextView 
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="法规遗漏" 
        />
</TableRow>    
<TableRow 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"  
    >
      <TextView 
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
        />
          <CheckBox 
    android:id="@+id/fagui_bug_box"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"   
        />
                          <TextView 
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="程序BUG" 
        />
        <CheckBox 
    android:id="@+id/fagui_suggest_box"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"   
        /> 
                                  <TextView 
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="程序功能建议" 
        /> 
</TableRow>
<EditText 
    android:id="@+id/suggest_edt"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:hint="swwwwffgghhbh"  
    />
</TableLayout>